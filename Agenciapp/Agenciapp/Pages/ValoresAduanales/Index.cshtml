@page
@model Agenciapp.Pages.ValoresAduanales.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ValorAduanal[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorAduanal[0].Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorAduanal[0].Um)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorAduanal[0].Value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorAduanal[0].Article)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorAduanal[0].Observaciones)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ValorAduanal) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Um)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Article)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observaciones)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ValorAduanalId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ValorAduanalId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ValorAduanalId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
