<section id="basic-form-layouts">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title" id="bordered-layout-colored-controls">Adicionar Oficina</h4>
                    <a class="heading-elements-toggle"><i class="fa fa-ellipsis-v font-medium-3"></i></a>
                    <div class="heading-elements">
                        <ul class="list-inline mb-0">
                            <li><a data-action="collapse"><i class="ft-minus"></i></a></li>
                            <li><a data-action="reload"><i class="ft-rotate-cw"></i></a></li>
                            <li><a data-action="expand"><i class="ft-maximize"></i></a></li>
                            <li><a data-action="close"><i class="ft-x"></i></a></li>
                        </ul>
                    </div>
                </div>
                <div class="card-body collapse in">
                    <div class="card-block">
                        <form class="form form-horizontal form-bordered" asp-action="Create">
                            <div class="form-body">
                                @*<h4 class="form-section"><i class="icon-eye6"></i>Datos de</h4>*@
                                <div class="row">
                                    <div class="col-md-12">

                                        <div class="form-group row last">
                                            <label class="col-md-3 label-control" for="Name">Nombre</label>
                                            @*<label asp-for="Name" class="col-md-3 control-label" for="userinput1"></label>*@
                                            <div class="col-md-9">
                                                <input asp-for="Name" class="form-control border-primary" placeholder="Nombre" />
                                                <span asp-validation-for="Name" class="text-danger"></span>
                                            </div>

                                        </div>
  
                                    </div>


                                </div>



                            
                            </div>

                            <div class="form-actions right">
                                <button type="button" class="btn btn-warning mr-1">
                                    <i class="ft-x"></i> Cancel
                                </button>
                                <button type="submit" class="btn btn-primary">
                                    <i class="fa fa-check-square-o"></i> Save
                                </button>
                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>

    @section Scripts {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }
