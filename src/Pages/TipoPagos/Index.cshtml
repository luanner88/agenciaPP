@page
@model Agenciapp.Pages.TipoPagos.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TipoPago[0].Type)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.TipoPago) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.TipoPagoId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.TipoPagoId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.TipoPagoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
